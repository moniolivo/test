    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "box-157-26-mp3-58-joyce-lieberman-palmist-reading-of-gloria-15-november-1980-canvas-1-index-annotation-4.json",
      "type": "Annotation",
